    "Datatype": "NUMBER",
    "Priority": 1,
    "REDCap Field name": "terapia_data_inizio",
    "Datediff": {
      "start": "data_diagnosi",
      "end": "terapia_data_inizio",
      "unit": "days"
    }
  },
  "STOP_DATE": {
    "Attribute type": "PATIENT",
    "Display name": "Stop Date",
    "Description": "The last date of the of the treatment(days)",
    "Datatype": "NUMBER",
    "Priority": 1,
    "REDCap Field name": "terapia_data_fine",
    "Datediff": {
      "start": "data_diagnosi",
      "end": "terapia_data_fine",
      "unit": "days"
    }
  },
  "STOP_REASON": {
    "Attribute type": "PATIENT",
    "Display name": "Stop Reason",
    "Description": "Reason for termination of treatment ",
    "Datatype": "STRING",
    "Priority": 1,
    "REDCap Field name": "motivo_fine_terapia",
    "Values": {
      "1": "Completion",
      "2": "Disease progression",
      "3": "Toxicity",
      "4": "Other"
    }
  },
  "RADIATION_THERAPY_DOSE": {
    "Attribute type": "PATIENT",
    "Display name": "Radiation therapy dose",
    "Description": "Radiation therapy administered dose (Gy)",
    "Datatype": "NUMBER",
    "Priority": 1,
    "REDCap Field name": "rt_dose_totale"
  },
  "OS_MONTHS": {
    "Attribute type": "PATIENT",
    "Display name": "Overall Survival (Months)",
    "Description": "Overall survival in months since initial diagonosis.",
    "Datatype": "NUMBER",
    "Priority": 1,
    "REDCap Field name": "fu_data",
    "Datediff": {
      "start": "data_diagnosi",
      "end": "fu_data",
      "unit": "months"
    }
  },
  "OS_STATUS": {
    "Attribute type": "PATIENT",
    "Display name": "Overall Survival Status",
    "Description": "Overall patient survival status.",
    "Datatype": "STRING",
    "Priority": 1,
    "REDCap Field name": "fu_stato",
    "Values": {
      "0": "0:LIVING",
      "1": "1:DECEASED"
    }
  },
  "DFS_MONTHS": {
    "Attribute type": "PATIENT",
    "Display name": "Disease Free (Months)",
    "Description": "Disease free in months since treatment",
    "Datatype": "NUMBER",
    "Priority": 1,
    "REDCap Field name": "fu_data",
    "Datediff": {
      "start": "terapia_data_inizio",
      "end": "fu_data",
      "unit": "months",
      "logic": "fu_stato == 1"
    }
  },
  "DFS_STATUS": {
    "Attribute type": "PATIENT",
    "Display name": "Disease Free Status",
    "Description": "Disease free status",
    "Datatype": "STRING",
    "Priority": 1,
    "REDCap Field name": "fu_stato_malattia",
    "Values": {
      "1": "0:DiseaseFree",
      "2": "1:Recurred/Progressed",
      "3": "1:Recurred/Progressed",
      "4": "1:Recurred/Progressed"
    }
  },
  "DATE_OF_DEATH": {
    "Attribute type": "PATIENT",
    "Display name": "Date of Death",
    "Description": "Date of Death",
    "Datatype": "STRING",
    "Priority": 1,
    "REDCap Field name": "decesso_data"
  },
  "PATIENT_DEATH_REASON": {
    "Attribute type": "PATIENT",
    "Display name": "Patient Death Reason",
    "Description": "Text term to identify the cause of death for a patient.",
    "Datatype": "STRING",
    "Priority": 1,
    "REDCap Field name": "decesso_causa",
    "Values": {
      "1": "Dead of Disease (DOD)",
      "2": "Dead of Other Cause (DOC)",
      "3": "Dead of Unknown Cause (DUC)"
    }
  }
}