    "Attribute type": "PATIENT",
    "Display name": "Treatment",
    "Description": "Treatment",
    "Datatype": "STRING",
    "Priority": 1,
    "REDCap Field name": "terapia_tipo",
    "Values": {
      "1": "Surgery",
      "2": "Radiotherapy",
      "3": "Chemotherapy"
    }
  },
